# Environment variables for the Ulixee Scraper application
# Copy this file to .env and fill in your values

# API Key for authentication (required)
API_KEY=your_secure_api_key_here

# Port settings (optional, defaults shown)
PORT=3000
CLOUD_PORT=1818

# Node environment
NODE_ENV=production

# Path to Chrome executable (use system Chrome instead of downloaded version)
ULIXEE_CHROME_EXECUTABLE_PATH=/usr/bin/google-chrome-stable